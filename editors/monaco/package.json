{
  "name": "monaco-jsona",
  "version": "0.1.2",
  "description": "Jsona plugin for monaco editor",
  "author": "Sigoden Huang <sigoden@gmail.com>",
  "type": "module",
  "types": "index.d.ts",
  "license": "MIT",
  "scripts": {
    "build:worker": "rollup -c rollup.config.worker.js",
    "build:index": "tsc -p tsconfig.build.json",
    "build": "npm run build:index && npm run build:worker",
    "prepublish": "npm run build",
    "prebuild": "rm -rf dist"
  },
  "keywords": [
    "monaco",
    "jsona"
  ],
  "files": [
    "jsona.worker.js",
    "index.js",
    "index.d.ts"
  ],
  "peerDependencies": {
    "monaco-editor": ">=0.30"
  },
  "dependencies": {
    "monaco-languageclient": "^3.0.1",
    "vscode-languageserver-protocol": "^3.17.2"
  },
  "devDependencies": {
    "@jsona/lsp": "^0.1.9",
    "@types/node": "^18.6.5",
    "@types/vscode": "^1.44.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "esbuild": "^0.14.45",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.8.2"
  }
}
